<% @title = 'New School Year' %>
<% @page_javascript = 'admin/school_years/new' %>

<div class="account-form">
  <%= error_messages_for :school_year %>

  <% form_for :school_year do |form| %>
    <fieldset>
        <legend>New School Year</legend>
        <span class="school-year-edit">
          <%= render :partial => 'school_year_form', :object => form %>
          <span style="color:red">*&nbsp;</span>Required Field
          <div>
            <%= submit_tag 'Create', :class => 'submit' %>
          </div>
        </span>
    </fieldset>
  <% end %>
</div>
